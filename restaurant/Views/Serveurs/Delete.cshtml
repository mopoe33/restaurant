@model restaurant.Models.Serveur

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_layoutDashboard.cshtml";
}


<div class="content_page">

    <div class="container">
        <h1>Supprimer Serveur</h1>


        <hr />
        <table>
            <tbody>
                <tr>
                    <th>Nom</th>
                    <td>@Html.DisplayFor(model => model.firstName) @Html.DisplayFor(model => model.lastName)</td>
                </tr>

                <tr>
                    <th>Heure Debut</th>
                    <td>@Html.DisplayFor(model => model.timeStart)</td>
                </tr>

                <tr>
                    <th>Heure Fin</th>
                    <td>@Html.DisplayFor(model => model.timeEnd)</td>
                </tr>






            </tbody>
        </table>
        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Supprimer" class="btn btn-danger" /> |
            <a asp-action="Index">Back to List</a>
        </form>
    </div>
</div>
<script src="~/js/main.js"></script>
